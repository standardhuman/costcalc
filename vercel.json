{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.mjs",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/diving/([^/]+\\.(css|js|mjs))", "dest": "/diving/$1" },
    { "src": "/diving", "dest": "/diving/diving.html" },
    { "src": "/admin", "dest": "/admin/admin.html" },
    { "src": "/inventory", "dest": "/inventory/inventory.html" },
    { "src": "/booking", "dest": "/booking.html" }
  ],
  "headers": [
    {
      "source": "/diving/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}