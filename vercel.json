{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "!(server|api)/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/quotes",
      "methods": ["GET", "POST"],
      "dest": "/server.js"
    },
    {
      "src": "/api/quotes/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|pdf))",
      "dest": "/$1"
    },
    {
      "src": "/quote/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/admin",
      "dest": "/server.js"
    },
    {
      "src": "/diving",
      "dest": "/server.js"
    },
    {
      "src": "/booking",
      "dest": "/server.js"
    },
    {
      "src": "/anode-quote",
      "dest": "/server.js"
    },
    {
      "src": "/comprehensive-quote",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}