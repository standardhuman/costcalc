{
  "watch": [
    "server.js",
    "*.html",
    "*.css",
    "*.js",
    "*.mjs",
    "admin.js",
    "script.js"
  ],
  "ext": "js,mjs,json,html,css",
  "ignore": [
    "node_modules/**",
    "*.test.js",
    "test-*.mjs",
    "*.spec.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "verbose": false
}